[
  {
    "command": "vscode-neovim.compositeEscape1",
    "key": "j",
    "when": "neovim.mode == insert && editorTextFocus",
    "args": "j"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen",
    "when": "(neovim.mode == normal && editorTextFocus || !editorIsOpen ) && !terminalFocus && !inQuickOpen"
  },
  {
    "key": "ctrl+i",
    "command": "editor.action.showHover",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+n",
    "command": "editor.action.goToDeclaration",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.navigateBack",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+g",
    "command": "editor.action.referenceSearch.trigger",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+u",
    "command": "editor.action.rename",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.nextEditorInGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.previousEditorInGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+shift+l",
    "command": "workbench.action.moveEditorToRightGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.focusRightGroup",
    "when": "!!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.moveEditorToRightGroup",
    "when": "!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+shift+h",
    "command": "workbench.action.moveEditorToLeftGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.focusLeftGroup",
    "when": "!!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.moveEditorToLeftGroup",
    "when": "!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+shift+j",
    "command": "workbench.action.moveEditorToBelowGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+j",
    "command": "workbench.action.focusBelowGroup",
    "when": "!!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+j",
    "command": "workbench.action.moveEditorToBelowGroup",
    "when": "!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+shift+k",
    "command": "workbench.action.moveEditorToAboveGroup",
    "when": "neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.focusAboveGroup",
    "when": "!!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.moveEditorToAboveGroup",
    "when": "!multipleEditorGroups && neovim.mode == normal && editorTextFocus"
  },
  {
    "key": "cmd+p",
    "command": "workbench.actions.view.toggleProblems"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "problemFocus"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "o",
    "command": "list.select",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "x",
    "command": "list.collapse",
    "when": "listFocus && treeElementCanCollapse && !inputFocus || listFocus && treeElementHasParent && !inputFocus"
  },
  {
    "key": "shift+x",
    "command": "list.collapseAll",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "listFocus && !inputFocus && workbench.explorer.openEditorsView.active"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "listFocus && !inputFocus"
  }
]
