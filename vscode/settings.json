{
  "files.insertFinalNewline": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Monokai",
  "workbench.colorCustomizations": {
    "editor.background": "#272822",
    "sideBar.background": "#272822",
    "terminal.background":"#272822",
    "terminal.foreground":"#FFFFFF",
    "terminalCursor.background":"#FFFFFF",
    "terminalCursor.foreground":"#999999",
    "terminal.ansiBlack":"#000000",
    "terminal.ansiBrightBlack":"#75715E",
    "terminal.ansiRed":"#C63467",
    "terminal.ansiBrightRed":"#F92672",
    "terminal.ansiGreen":"#9CC533",
    "terminal.ansiBrightGreen":"#A6E22E",
    "terminal.ansiYellow":"#C3B96E",
    "terminal.ansiBrightYellow":"#E6DB74",
    "terminal.ansiMagenta":"#8664C5",
    "terminal.ansiBrightMagenta":"#AE81FF",
    "terminal.ansiBlue":"#C8841F",
    "terminal.ansiBrightBlue":"#FD971F",
    "terminal.ansiCyan":"#5FB7C6",
    "terminal.ansiBrightCyan":"#66D9EF",
    "terminal.ansiWhite":"#BFBFBF",
    "terminal.ansiBrightWhite":"#E5E5E5"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "Myrica M",
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": false,
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "castwide.solargraph"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "terminal.integrated.fontSize": 14,
  "git.openRepositoryInParentFolders": "always",
  "scrolloff.scrolloff": 5,
  "indentRainbow.colors": [
    "rgba(236,236,236,0)",
    "rgba(236,236,236,0.03)"
  ],
  "whitespacepp.renderLF": false,
  "whitespacepp.renderEOF": false,
  "whitespacepp.foreColorTAB": "#646464",
  "whitespacepp.foreColorWS": "#d3201f",
  "whitespacepp.textTAB": ".",
  "trailing-spaces.backgroundColor": "rgba(211,32,31)",
  "trailing-spaces.borderColor": "rgba(255,238,153)",
  "trailing-spaces.syntaxIgnore": [
    "markdown"
  ],
  "trailing-spaces.highlightCurrentLine": false,
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.locate": {
    "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}",
    "include": "**/*.rb"
  },
  "ruby.useLanguageServer": true,
  "solargraph.formatting": true,
  "solargraph.autoformat": true,
  "solargraph.diagnostics": true,
  "vscode-neovim.compositeKeys": {
    "jj": {
        "command": "vscode-neovim.escape"
    }
  }
}
