{
  "files.insertFinalNewline": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Monokai Dark Soda",
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "Myrica M",
  "editor.fontSize": 14,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 14,
  "scrolloff.scrolloff": 5,
  "indentRainbow.colors": [
    "rgba(236,236,236,0)",
    "rgba(236,236,236,0.03)"
  ],
  "whitespacepp.renderLF": false,
  "whitespacepp.renderEOF": false,
  "whitespacepp.foreColorTAB": "#646464",
  "whitespacepp.foreColorWS": "#d3201f",
  "whitespacepp.textTAB": ".",
  "trailing-spaces.backgroundColor": "rgba(211,32,31)",
  "trailing-spaces.borderColor": "rgba(255,238,153)",
  "trailing-spaces.syntaxIgnore": [
    "markdown"
  ],
  "trailing-spaces.highlightCurrentLine": false,
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "ruby.intellisense": "rubyLocate",
  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": true
  },
  "ruby.format": "rubocop",
  "git.openRepositoryInParentFolders": "always"
}
